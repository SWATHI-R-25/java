///////
public class Main {
    public static void main(String[] args) {

        String a = "hello";
        String b = "hello";
        String c = new String("hello"); // creates new memory

        System.out.println(a == b);       
        System.out.println(a == c);        

        System.out.println(a.equals(b));   
        System.out.println(a.equals(c));   
    }
}                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                                     
