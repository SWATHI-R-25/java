//add at last,pos
class Node {
    Node prev;
    int data;
    Node next;

    Node(int data) {
        this.data = data;
        prev = null;
        next = null;
    }
}

class Doublylinkedlist {
    Node head = null;
    Node tail = null;
    int size = 0;

    // Add at beginning
    void addatbeg(int data) {
        Node newnode = new Node(data);

        if (head == null) {
            head = tail = newnode;
        } else {
            newnode.next = head;
            head.prev = newnode;
            head = newnode;
        }
        size++;
    }

    // Add at last
    void addatlast(int data) {
        Node newnode = new Node(data);

        if (head == null) {
            head = tail = newnode;
        } else {
            tail.next = newnode;
            newnode.prev = tail;
            tail = newnode;
        }
        size++;
    }

    // Add at position
    void addatpos(int data, int pos) {

        if (pos < 1 || pos > size + 1) {
            System.out.println("Invalid Position");
            return;
        }

        if (pos == 1) {
            addatbeg(data);
            return;
        }

        if (pos == size + 1) {
            addatlast(data);
            return;
        }

        Node newnode = new Node(data);
        Node temp = head;

        for (int i = 1; i < pos - 1; i++) {
            temp = temp.next;
        }

        newnode.next = temp.next;
        newnode.prev = temp;
        temp.next.prev = newnode;
        temp.next = newnode;

        size++;
    }

    // Display forward
    void display() {
        Node temp = head;
        while (temp != null) {
            System.out.print(temp.data + " ");
            temp = temp.next;
        }
        System.out.println();
    }
}

public class Main {
    public static void main(String[] args) {

        Doublylinkedlist d = new Doublylinkedlist();

        d.addatbeg(100);
        d.display();
        d.addatlast(200);
        d.display();
        d.addatlast(300);
        d.display();
        d.addatpos(120, 2);

        d.display();
    }
}
