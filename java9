//delete at pos
//    https://www.onlinegdb.com/GvZOLio3f
void deleteatpos(int pos){
  if(pos<=0 || pos>size){
    System.out.println("Invalid 
");
    return;
  }
  if(pos==1){
    deleteatbeg();
    return;
  }
  if(pos==size){
    deleteatlast();
    return;
  }
  Node temp=head;
  for(int i=1;i<pos-1;i++){
    temp=temp.next;
  }
  Node temp1=temp.next;
  Node.next=temp1.next;
  temp.next.prev=temp;
size--;
}
//searching
void search(int d){
  if(head==null){
    System.out.println("There is no data to search");
    return;
  }
  Node temp = head;
  while(temp!=null){
    if(temp.data==d){
      System.out.println("Data Found")
      return;
    }
    temp=temp.next;
  }
  System.out.println("There is no data");
}
//updation
void updation(int pos,int d){
  if(pos<=0 || pos>size){
    System.out.println("Invalid pos");
    return;
  }
  if(pos==1){
    head.data=d;
    return;
  }
  if(pos=size){
    tail.data=d;
    return;
  }
  Node temp=head;
  for(int i=1;i<pos;i++){
    temp=temp.next;
  }
  		temp.data=d;

	}
