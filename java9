//circular linked list
//singly circular lined list
import java.util.*;

class Node {
    int data;
    Node next;

    Node(int data) {
        this.data = data;
        next = null;
    }
}

class CircularSinglyLinkedList {
    Node head = null;
    Node tail = null;
    int size = 0;

    // Add at last
    void addAtLast(int data) {
        Node newnode = new Node(data);
        size++;

        if (head == null) {
            head = tail = newnode;
            tail.next = head;
        } else {
            tail.next = newnode;
            tail = newnode;
            tail.next = head;
        }
    }

    // Add at beginning
    void addAtBeg(int data) {
        Node newnode = new Node(data);
        size++;

        if (head == null) {
            head = tail = newnode;
            tail.next = head;
        } else {
            newnode.next = head;
            head = newnode;
            tail.next = head;
        }
    }

    // Display
    void display() {
        if (head == null) {
            System.out.println("There is no element to print");
            return;
        }

        Node temp = head;
        do {
            System.out.print(temp.data + " ");
            temp = temp.next;
        } while (temp != head);

        System.out.println();
    }
}

public class Main {
    public static void main(String[] args) {
        CircularSinglyLinkedList c = new CircularSinglyLinkedList();
        c.addAtLast(10);
        c.addAtLast(20);
        c.addAtLast(30);
        c.addAtBeg(40);
        c.display();
    }
}

//delete at last
void deleteatlast {
        if (head == null) {
            System.out.println(" no element to display");
            return;
        }
        if(head==tail){
          head=tail=null;
          System.out.println("All elements are deleted");
          
        }
        Node temp=head;
        while(temp.next!=tail){
          temp=temp.next;
        }
        tail=temp;
        tail.next=head;
        size--;
    }
// delete at beginning
 void deleteatbeg() {
        if (head == null) {
            System.out.println(" no element to display");
            return;
        }
        if(head==tail){
          head=tail=null;
          System.out.println("All elements are deleted");
          
        }
        head=head.next;
        tail.next=head;
        size--;
    }
